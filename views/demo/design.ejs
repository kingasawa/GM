<div class="container">
  <!-- Headings & Paragraph Copy -->
  <div class="row">
    <div class="col-xs-12">
      <h3>Color</h3>
      <div class="btn-toolbar chose-color" role="toolbar">

        <div class="btn-group" role="group" title="White">
          <button class='btn' style="background-color:#ffffff;">
            <i class="fa fa-exchange" aria-hidden="true"></i>
          </button>
        </div>
        <div class="btn-group" role="group" title="Cherry Red">
          <button class='btn' style="background-color:#c50404;">
            <i class="fa fa-exchange" aria-hidden="true"></i>
          </button>
        </div>
        <div class="btn-group" role="group" title="Yellow">
          <button class='btn' style="background-color:yellow;">
            <i class="fa fa-exchange" aria-hidden="true"></i>
          </button>
        </div>


      </div>
    </div>
    <div class="col-xs-12">
      <h3>Material</h3>
      <% for (var item = 0; item < material.length; item++) { %>
        <img class="material-icon" data-id="<%= material[item]%>" src="http://img.gearment.com/unsafe/0x120/<%= material[item]%>"></img>
      <%}%>

    </div>
    <div class="col-xs-12">
      <h3>Your Uploaded</h3>
      <form action="/uploader" class="dropzone" id="logoUploader"></form>
      <div class="uploaded-logo">
        <% for (var item = 0; item < logo.length; item++) { %>
          <img data-id="<%= logo[item]%>" class="img-logo"
             src="http://img.gearment.com/unsafe/fit-in/100x100/<%= logo[item]%>">
        <%}%>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12" id="imageeditor">
      <div class="btn-group">
        <h4>Image Toolbar</h4>
        <button id="remove-selected" title="Delete selected item"><i class="fa fa-trash"></i> Delete logo</button>
      </div>
    </div>
    <div class="col-xs-12">
      <div id="shirtDiv" class="page">
        <img id="tshirtFacing" data-id="3b70c726a2b14e1380d385de8078a9e7" src="http://img.gearment.com/unsafe/0x500/3b70c726a2b14e1380d385de8078a9e7"></img>
        <div id="drawingArea">
          <canvas id="tcanvas" width=375 height="500" class="hover" style="-webkit-user-select: none;top: 0"></canvas>
        </div>
      </div>
    </div>
    <div class="col-xs-12">
      <h4>Backend Render Result</h4>
      <p class="well text-center" id="renderImageUrl">
        <img/>

        <div>
          <a id="urlResult"></a>
        </div>

      </p>
    </div>
  </div>

</div>


