
<div class="container" id="admin-page">
  <aside class="sidenav">
    <%- partial ('menu-side.ejs') %>
  </aside>

  <div class="main-content">
    <div class="head-content">
      <a class="menu-button" href="#">
        <div class="menu-arrow aright text-center">
          <i class="fa fa-chevron-right pull-left"></i>
        </div>
      </a>
      <a class="menu-button" href="#">
        <div class="sr-only menu-arrow aleft text-center">
          <i class="fa fa-chevron-left pull-left"></i>
        </div>
      </a>
      <div class="menu-title">
        <h4><span class="name-param">ScanForm</span></h4>
      </div>
    </div>

    <div class="detail-content" id="scanform-page">
      <ul class="breadcrumb">
        <li><a href="/acp"><i class="fa fa-home"></i> Home</a></li>
        <li class="active">ScanForm</li>
      </ul>


      <table class="table table-hover">
        <thead>
        <tr>
          <th>View</th>
          <th>ScanFormID</th>
          <th>Service</th>
          <th>URL</th>
          <th>Date</th>
          <th>Action</th>
        </tr>
        </thead>
        <tbody>
        <% _.each(foundScanForm, function(scanform) { %>
        <tr>
          <td><%= scanform.id %></td>
          <td><%= scanform.scan_form_id %></td>
          <td><%= scanform.form_type %></td>
          <td><a href="<%= scanform.form_url %>" download>Download</a></td>
          <td><%= sails.moment(scanform.createdAt).format('MM/DD/YYYY') %></td>
          <td>
            <% if (scanform.status == 'created') { %>
            <a data-id="<%= scanform.batch_id %>" type="button" class="create_manifest btn btn-primary"><i class="fa fa-pencil-square-o"></i> Create Manifest</a>
            <% } else { %>
            <span class="text-success">Created</span>
            <% } %>
          </td>
        </tr>
        <% }) %>


        </tbody>
      </table>
    </div>





  </div>
</div>
