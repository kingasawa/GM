
<div class="container" id="admin-page">
  <aside class="sidenav">
    <%- partial ('menu-side.ejs') %>
  </aside>

  <div class="main-content">
    <div class="head-content">
      <a class="menu-button" href="#">
        <div class="menu-arrow aright text-center">
          <i class="fa fa-chevron-right pull-left"></i>
        </div>
      </a>
      <a class="menu-button" href="#">
        <div class="sr-only menu-arrow aleft text-center">
          <i class="fa fa-chevron-left pull-left"></i>
        </div>
      </a>
      <div class="menu-title">
        <h4><span class="name-param">Manifest</span></h4>
      </div>
    </div>

    <div class="detail-content" id="scanform-page">
      <ul class="breadcrumb">
        <li><a href="/acp"><i class="fa fa-home"></i> Home</a></li>
        <li class="active">Manifest</li>
      </ul>


      <table class="table table-hover">
        <thead>
        <tr>
          <th>View</th>
          <th>BatchID</th>
          <th>Number Shipments</th>
          <th>State</th>
          <th>Date</th>
        </tr>
        </thead>
        <tbody>
        <% _.each(foundManifest, function(manifest) { %>
        <tr>
          <td><%= manifest.id %></td>
          <td><%= manifest.batch_id %></td>
          <td><%= manifest.num_shipments %></td>
          <td><%= manifest.state %></td>
          <td><%= sails.moment(manifest.createdAt).format('MM/DD/YYYY') %></td>
        </tr>
        <% }) %>


        </tbody>
      </table>
    </div>





  </div>
</div>
