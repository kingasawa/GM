<div style="margin-top:90px" class="modal fade modal-warning" id="mySavingModal" role="dialog">
  <div class="modal-dialog modal-sm">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title"><i class="fa fa-cloud"></i> Save Settings</h4>
      </div>
      <div class="modal-body">
        <form id="save-campaign">
          <div class="form-group">
            <label for="save-name">Name</label>
            <input type="text" class="input-lg form-control" minlength="6" required name="save-name" id="save-name"
                   value="<%=_.get(getSave, 'savingName', '')%>"
            >
          </div>
          <button type="submit" class="btn btn-primary btn-block">Save</button>
        </form>
      </div>
    </div>
  </div>
</div>
